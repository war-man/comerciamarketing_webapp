
@{
    ViewBag.Title = "Comercia Marketing - Analytics";
}
<!-- partial -->
<div class="container-fluid page-body-wrapper">
    <div class="main-panel">
        <div class="content-wrapper">
            <div class="container">
               
                <!--RESOURCES-->
                <div class="row">
                    <div class="col-md-12 grid-margin">
                        <div class="card">
                            <div class="card-body">
                                

                                @foreach (var resource in ViewBag.recursos) {
                                    if (resource.ID_tiporecurso == 1)
                                    {
                                        <h4 class="card-title">Merchandising</h4>
                                    }
                                    else {
                                        <h4 class="card-title">Demos</h4>
                                    }


                                    <div id="iframePowerBI" style="height:750px; position:relative">
                                        <iframe style="border:none; width:100%; height:700px; z-index:1" src="@resource.url" frameborder="0" allowfullscreen="false"></iframe>
                                        <!--<img style="top:0px; right:0px; position:absolute;width:100%; height:670px; z-index:9; opacity:0" src="~/Content/images/MechandiserWeb.jpg">-->

                                        <img style="top:663px; left:0px; position:absolute; z-index:9" src="~/Content/images/iframefooter.png" width="200" height="37">
                                        <img style="top:663px; right:0px; position:absolute; z-index:9" src="~/Content/images/iframefooter.png" width="200" height="37">
                                    </div>



                                }

                            </div>
                        </div>
                    </div>
                </div>
                @Html.Partial("~/Views/Shared/Footer2.cshtml")
            </div>

        </div>
    </div>
    <!-- content-wrapper ends -->
    <!-- partial:../../partials/_footer.html -->
    <!-- partial -->
</div>
<!-- main-panel ends -->
