@model comerciamarketing_webapp.Models.historial_conexiones

@{
    ViewBag.Title = "Comercia Marketing";
}

<div class="container single_product_container">
    <div class="row">
        <div class="col">

            <!-- Breadcrumbs -->

            <div class="breadcrumbs d-flex flex-row align-items-center">
                <ul>
                    <li><a href="@Url.Action("Main", "Home")">Home</a></li>
                    @if (ViewBag.modulo == "customers")
                    {
                        <li><a href="@Url.Action("Index", "Empresas")"><i class="fa fa-angle-right" aria-hidden="true"></i>Customers</a></li>
                    }
                    else {
                        <li><a href="@Url.Action("Demo_users", "Home")"><i class="fa fa-angle-right" aria-hidden="true"></i>Demos</a></li>
                    }
                    <li class="active"><a href="#"><i class="fa fa-angle-right" aria-hidden="true"></i>Users</a></li>
                    <li class="active"><a href="#"><i class="fa fa-angle-right" aria-hidden="true"></i>Login History</a></li>
                </ul>
            </div>

    
                <h4>Details</h4>
                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        Date: @Html.DisplayFor(model => model.fecha_conexion)
                    </dt>
                    <dt>
                        IP: @Html.DisplayFor(model => model.ip)
                    </dt>

                    <dt>
                        Hostname: @Html.DisplayFor(model => model.hostname)
                    </dt>
                    <dt>
                        Type: @Html.DisplayFor(model => model.typeh)
                    </dt>
                    <dt>
                        Continent: @Html.DisplayFor(model => model.continent_name)
                    </dt>
                    <dt>
                        Country code: @Html.DisplayFor(model => model.country_code)
                    </dt>
                    <dt>
                        Country name: @Html.DisplayFor(model => model.country_name)
                    </dt>
                    <dt>
                        Region code: @Html.DisplayFor(model => model.region_code)
                    </dt>
                    <dt>
                        Region name: @Html.DisplayFor(model => model.region_name)
                    </dt>
                    <dt>
                        City: @Html.DisplayFor(model => model.city)
                    <dt>
                        Latitude: @Html.DisplayFor(model => model.latitude)
                    </dt>
                    <dt>
                        Longitude: @Html.DisplayFor(model => model.longitude)

                    </dt>


                </dl>
           
            <p>
                @Html.ActionLink("Back", "Login_history", new { id = ViewBag.ID_usuarioback, modulo=ViewBag.modulo })
            </p>
            </div>
</div>
</div>