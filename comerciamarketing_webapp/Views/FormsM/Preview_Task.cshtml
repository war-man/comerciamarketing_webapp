                                <input type="number" onClick="this.select();" min="0" inputmode="numeric" pattern="[0-9]*" value="@objeto.fvalue" class="form-control" id="@objeto.idkey" />
                            </div>
                        </div>
                    </div>
                </div>
            }
            <!--PARA GIFT -->
